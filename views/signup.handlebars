<div class="container">
  <h1>Sign Up</h1>
<form>
  <div class="mb-3">
    <label for="usernameInput" class="form-label">Username</label>
    <input type="text" class="form-control" id="usernameSignInput" aria-describedby="usernameHelp">
  </div>
  <div class="mb-3">
    <label for="passwordInput" class="form-label">Password</label>
    <input type="password" class="form-control" id="passwordSignInput">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>

</form>
</div>

<script src="/js/signup.js"></script>